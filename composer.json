{
    "name": "madmatt/silverstripe-uuid",
    "description": "A module that allows for easy usage of UUIDs on Silverstripe DataObjects",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",

    "keywords": [
        "silverstripe",
        "cms",
        "uuid"
    ],

    "require": {
        "silverstripe/framework": "^4",
        "ramsey/uuid": "^4"
    },

    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },

    "autoload": {
        "psr-4": {
            "Madmatt\\UUID\\": "src/",
            "Madmatt\\UUID\\Tests\\": "tests/"
        }
    }
}
